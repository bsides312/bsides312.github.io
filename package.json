{
	"name": "bsides312-website",
	"description": "BSides312 - Chicago's Biggest Little Non-Profit Hacking & Information Security Conference",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"serve": "python3 -m http.server 8000",
		"serve-alt": "npx http-server -p 8000"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"terser": "^5.43.1",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^7.0.4"
	},
	"keywords": [
		"bsides",
		"cybersecurity",
		"conference",
		"chicago",
		"hacking",
		"infosec"
	],
	"author": "BSides312 Team",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/bsides312/bsides312.github.io.git"
	},
	"homepage": "https://bsides312.org"
}
